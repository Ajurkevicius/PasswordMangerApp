<Window x:Class="ImageRecognition.UIWindows.InputMasterPasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageRecognition.UIWindows"
        mc:Ignorable="d"
        Title="InputMasterPasswordWindow" Height="600" Width="900"
         Background="#1d1e2c"
         WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBox Margin="250,258,250,301">
            <TextBox.Resources>
                <Style TargetType="{x:Type Border}">
                    <Setter Property="CornerRadius" Value="5"/>
                    <Setter Property="Background" Value="#B5B5B5"/>
                </Style>
            </TextBox.Resources>
        </TextBox>
        <Button Content="Continue" Margin="595,365,153,181" Background="#00CC66">
            <Button.Resources>
                <Style TargetType="{x:Type Border}">
                    <Setter Property="CornerRadius" Value="10"/>
                </Style>
            </Button.Resources>
        </Button>

        <Label Content="Input master password" Margin="324,152,324,390" FontSize="24" Foreground="White"/>
    </Grid>
</Window>
